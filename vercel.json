{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
  
       "routes": [
           { "handle": "filesystem" },
            {
        "src": "/[^.]+",
        "dest": "/",
        "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
        "status": 200
      },
      {
        "src": "/(.*)",
        "status": 404,
        "dest": "/"
      }
    ]
    
  }
